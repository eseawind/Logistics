<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    			"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    			"http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
    <constant name="struts.i18n.encoding" value="UTF-8"/>
    <package name="Warehouse" extends="json-default">
    	<action name="Warehouse.delete" class="Logistics.Servlet.WarehouseAction" method="delete">
    		<result type="json">
    		<param name="includeProperties">
    		message,success,valid
    		</param>
    		</result>
    	</action>
    	
    	<action name="Warehouse.update" class="Logistics.Servlet.WarehouseAction" method="update">
    		<result type="json">
    		<param name="includeProperties">
    		message,success,valid
    		</param>
    		</result>
    	</action>
    	
    	<action name="Warehouse.queryOne" class="Logistics.Servlet.WarehouseAction" method="queryOne">
    		<result type="json">
    		<param name="includeProperties">
    		message,success,valid,data.*
    		</param>
    		</result>
    	</action>
    	
    	<action name="Warehouse.insert" class="Logistics.Servlet.WarehouseAction" method="insert">
    		<result type="json">
    		<param name="includeProperties">
    		message,success,valid
    		</param>
    		</result>
    	</action>
    	
    	<action name="Warehouse.queryOnCondition" class="Logistics.Servlet.WarehouseAction" method="queryOnCondition">
    		<result type="json">
    		<param name="includeProperties">
    		message,success,valid,resultMapList.*,qualifiedAmount
    		</param>
    		</result>
    	</action>
    	<action name="Warehouse.queryNames" class="Logistics.Servlet.WarehouseAction" method="queryNames">
    		<result type="json">
    		<param name="includeProperties">
    		message,success,valid,resultMapList.*,qualifiedAmount
    		</param>
    		</result>
    	</action>
    	
    </package>
    
    
    
    
</struts>
