<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    			"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    			"http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
    <constant name="struts.i18n.encoding" value="UTF-8"/>
    <package name="StockoutManifest" extends="json-default">
    
    	
    	<action name="StockoutManifest.delete" class="Logistics.Servlet.StockoutManifestAction" method="delete">
    		<result type="json">
    		<param name="includeProperties">
    		message,success,valid
    		</param>
    		</result>
    	</action>
    	<action name="StockoutManifest.approve" class="Logistics.Servlet.StockoutManifestAction" method="approve">
    		<result type="json">
    		<param name="includeProperties">
    		message,success,valid
    		</param>
    		</result>
    	</action>
    	<action name="StockoutManifest.audit" class="Logistics.Servlet.StockoutManifestAction" method="audit">
    		<result type="json">
    		<param name="includeProperties">
    		message,success,valid
    		</param>
    		</result>
    	</action>
    	
    	<action name="StockoutManifest.update" class="Logistics.Servlet.StockoutManifestAction" method="update">
    		<result type="json">
    		<param name="includeProperties">
    		message,success,valid
    		</param>
    		</result>
    	</action>
    	
    	<action name="StockoutManifest.queryOne" class="Logistics.Servlet.StockoutManifestAction" method="queryOne">
    		<result type="json">
    		<param name="includeProperties">
    		message,success,valid,data.*,resultMapList.*
    		</param>
    		</result>
    	</action>
    	
    	<action name="StockoutManifest.insert" class="Logistics.Servlet.StockoutManifestAction" method="insert">
    		<result type="json">
    		<param name="includeProperties">
    		message,success,valid
    		</param>
    		</result>
    	</action>
    	
    	<action name="StockoutManifest.queryOnCondition" class="Logistics.Servlet.StockoutManifestAction" method="queryOnCondition">
    		<result type="json">
    		<param name="includeProperties">
    		message,success,valid,resultMapList.*,qualifiedAmount
    		</param>
    		</result>
    	</action>
    	
    	<action name="StockoutManifest.queryItems" class="Logistics.Servlet.StockoutManifestAction" method="queryItems">
    		<result type="json">
    		<param name="includeProperties">
    		message,success,valid,data.*,resultMapList.*
    		</param>
    		</result>
    	</action>
    	
    	<action name="StockoutManifest.queryCustomerQuote" class="Logistics.Servlet.StockoutManifestAction" method="queryCustomerQuote">
    		<result type="json">
    		<param name="includeProperties">
    		message,success,valid,data.*
    		</param>
    		</result>
    	</action>
    	
    	
    </package>
    
    
    
    
</struts>
